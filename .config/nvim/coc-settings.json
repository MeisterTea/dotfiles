{
  "javascript.validate.enable": false,
  "snippets.extends": {
    "javascriptreact": ["javascript", "css"],
    "javascript": ["javascript", "css"]
  },
  "languageserver": {
    "intelephense": {
      "command": "intelephense",
      "args": ["--stdio"],
      "filetypes": ["php"],
      "initializationOptions": {
        "storagePath": "/tmp/intelephense"
      }
    }
  }
}
